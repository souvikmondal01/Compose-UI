<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ComposeUI"
        tools:targetApi="31">
        <activity
            android:name=".onboarding_screen_ui.OnboardingActivity"
            android:exported="false"
            android:label="@string/title_activity_onboarding"
            android:theme="@style/Theme.ComposeUI" />
        <activity
            android:name=".banking_app_ui.BankingAppUIActivity"
            android:exported="false"
            android:label="@string/title_activity_banking_app_uiactivity"
            android:theme="@style/Theme.ComposeUI" />
        <activity
            android:name=".recipes_app_ui.RecipeUIActivity"
            android:exported="false"
            android:label="@string/title_activity_recipe_uiactivity"
            android:theme="@style/Theme.ComposeUI" />
        <activity
            android:name=".pizza_app_ui.PizzaAppActivity"
            android:exported="false"
            android:label="@string/title_activity_pizza_app"
            android:theme="@style/Theme.ComposeUI" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.ComposeUI">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>